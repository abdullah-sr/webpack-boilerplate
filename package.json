{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --progress --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack -p --progress --config webpack.config.js"
  },
  "author": "",
  "license": "MIT",
  "babel": {
    "presets": [
      "env",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": [
      "airbnb"
    ],
    "rules": {
      "linebreak-style": 0,
      "indent": [
        "error",
        4
      ],
      "no-console": "off"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.4",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.0.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.0"
  }
}
